{% set JSONraw %}{% include "../wordpress-posts/footer-data" + tags | langFilePostfix + ".json" %}{% endset %}
{% set JSON = JSONraw | jsonparse %}
{% set FooterLinks1 = JSON.Table1 %}
{% set FooterLinks2 = JSON.Table2 %}
{% set Terms = JSON.Table3 %}

<div class="container-fluid">
  <div class="container bg-primary text-white pt-3 pb-3">
    <div class="row feedback-form">
      <div class="col-md-5">
        <label>Is this page useful?</label>

        <button class="ml-5 btn btn-sm btn-primary btn-bd-primary underline" id="feedback-yes">Yes</button>
        <button class="ml-5 btn btn-sm btn-primary btn-bd-primary underline" id="feedback-no">No</button>
      </div>
      
      <div class="col-md-7">
      <label for="add-feedback">Additional feedback:</label>
      <textarea name="add-feedback text-right w-100" id="add-feedback"  rows="2" cols="50"></textarea>
      <div class="feedback-error d-none small-text mt-0 mb-3">This field required</div>
      <button class="btn d-none btn-outline-light" type="submit" id="feedback-submit">Submit</button>
      </div>
    </div>
    <div class="row d-none feedback-thanks">
      <div class="col-md-12">
         Thank you for your feedback!
      </div>
    </div>
  </div>
</div>

<div class="footer mt-0 alpha-footer">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
          <ul class="list-group list list-group-horizontal-lg flex-fill list-group-flush">
            {% for item in FooterLinks1 %}
            <li class="list-group-item alpha-footer"><a href="{{ item.URL }}">{{ item.Name | safe }}</a></li>
            {% endfor %}
          </ul>
          <p><strong>{{Terms[7].Text | safe}}</strong>: <a href="tel:18334224255">1-833-422-4255</a> {{Terms[8].Text | safe}}</p>
          <p class="text-secondary">{{Terms[0].Text | safe}}</p>
      </div>
    </div>
  </div>
</div>

<div class="alphabar">
  <div class="container text-right text-center-mobile">
    <p>
      <a class="alphabar-link" href="{{FooterLinks2[0].URL}}">{{FooterLinks2[0].Name | safe}}</a>
      <a class="alphabar-link" href="{{FooterLinks2[1].URL}}">{{FooterLinks2[1].Name | safe}}</a>
      <a class="alphabar-link" href="{{FooterLinks2[2].URL}}">{{FooterLinks2[2].Name | safe}}</a>
    </p>
  </div>
</div>

{%- pagesection page, "foo" %}
<!-- Using this to make sure certain classes are included in the CSS build.  Should not ever render. -->
<a href="" class="list-group-item list-group-item-action guide active d-none"></a>
{% endpagesection -%}

<script type="module">{% include "built.js" %}</script>
