{% set JSONraw %}{% include "../wordpress-posts/footer-data" + tags | langFilePostfix + ".json" %}{% endset %}
{% set JSON = JSONraw | jsonparse %}
{% set Terms = JSON.Table3 %}
{% set langwptag = (tags | langRecord).wptag %}
{% set homelinkpath = "/" + (tags | langRecord).pathpostfix %}
{% set homeLink = "https://covid19.ca.gov" + homelinkpath %}


<header>
  <a class="skippy sr-only-focusable text-center" href="#main">
    <span class="skippy-text">{{Terms[1].Text}}</span>
  </a>
  <cwds-survey data-npi-survey-url="{{ tags | npiSurveyUrl | safe }}" data-pulse-survey-url="{{ tags | pulseSurveyUrl | safe }}" data-survey-prompt="{{Terms[9].Text}}" data-pulse-survey-prompt="{{Terms[10].Text}}"></cwds-survey>
  <script type="module">
    {% include "survey.js" %}
  </script>
  <nav id="top" class="navbar navbar-expand navbar-dark bd-navbar navbar-inverse px-0 pt-2 pb-3">
    <div class="container flex-wrap flex-lg-nowrap py-0 px-3 pr-0">

<a class="header-logo" href="/">
			<div class="header-logo-grid">
				<div class="header-logo-mark">
					<svg class="logo-mark" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 140"><title>California for All</title><g class="mark-ca"><path class="logo-mark-orange" d="M48.2,83l-13.2,28.6h25.2L48.2,83z M66.7,126.9H28.1l-6,13H0.2l48.7-99.8l44.6,99.8H72.1L66.7,126.9z"></path><polygon class="logo-mark-orange" points="117,42.9 117,122.4 146.9,122.4 146.9,139.9 97.1,139.9 97.1,42.9"></polygon><polygon class="logo-mark-orange" points="170.2,42.9 170.2,122.4 200,122.4 200,139.9 150.2,139.9 150.2,42.9"></polygon></g>
					<g class="mark-all"><path class="logo-mark-white" d="M21,8.7c-2-2.2-4.3-3.3-7.1-3.3c-4.6,0-8.4,3.9-8.4,8.7c0,4.8,3.7,8.7,8.4,8.7c2.9,0,5.3-1.2,7.4-3.6l3.4,3.4 c-2.8,3.1-6.6,5.1-10.8,5.1C6.2,27.8,0,21.7,0,14.1C0,6.5,6.2,0.4,13.9,0.4c4.2,0,8.1,2,10.8,5.1L21,8.7z"></path><path class="logo-mark-white" d="M37,11.8l-3.6,7.9h6.9L37,11.8z M42,23.9H31.4l-1.6,3.6h-6L37.1,0l12.3,27.5h-5.9L42,23.9z"></path><polygon class="logo-mark-white" points="56.1,0.8 56.1,22.7 64.3,22.7 64.3,27.5 50.6,27.5 50.6,0.8"></polygon><rect class="logo-mark-white" x="66" y="0.8" width="5.5" height="26.7"></rect><polygon class="logo-mark-orange" points="74.6,27.5 74.6,0.8 89.1,0.8 89.1,5.6 80.1,5.6 80.1,11.1 87.6,11.1 87.6,15.9 80.1,15.9 80.1,27.5"></polygon><path class="logo-mark-orange" d="M95.1,14.1c0,4.8,3.7,8.7,8.4,8.7c4.6,0,8.4-3.9,8.4-8.7c0-4.8-3.8-8.7-8.4-8.7C98.8,5.4,95.1,9.3,95.1,14.1 M117.4,14.1c0,7.6-6.2,13.7-13.9,13.7c-7.7,0-13.9-6.1-13.9-13.7c0-7.6,6.2-13.7,13.9-13.7C111.2,0.4,117.4,6.5,117.4,14.1"></path><path class="logo-mark-orange" d="M125.3,5.6v6.7h2.9c2.3,0,3.9-1.1,3.9-3.4c0-2.2-1.6-3.3-3.9-3.3H125.3z M125.3,16.8v10.6h-5.5V0.8h8.8 c4.9,0,9,3.1,9,8c0,3.1-1.7,5.7-4.2,7l5,11.6h-6L128,16.8H125.3z"></path><polygon class="logo-mark-white" points="159.4,0.8 164.9,0.8 164.9,28.2 145.9,11.6 145.9,27.5 140.4,27.5 140.4,0 159.4,16.6"></polygon><rect x="167.3" y="0.8" class="logo-mark-white" width="5.5" height="26.7"></rect><path class="logo-mark-white" d="M187.2,11.8l-3.6,7.9h6.9L187.2,11.8z M192.3,23.9h-10.6l-1.6,3.6h-6L187.4,0l12.3,27.5h-5.9L192.3,23.9z"></path></g></svg>
				</div>
				<div class="header-logo-text">
					<div class="header-logo-text-url">COVID19.CA.GOV</div>
					<div class="header-logo-text-title">Your Actions Save Lives</div>
				</div>
			</div>
		</a>

<ul class="utility-nav navbar-nav ml-sm-auto mobile">
        
        <li class="nav-item mr-0 pr-0 mt-3">
          <form action="{{homelinkpath}}search/" class="search-form">
            <input name="q" type="search" placeholder="{{Terms[3].Text}}" class="header-search-field">
            <button class="search-header-btn"><span class="ca-gov-icon-search" aria-hidden="true"></span><span class="sr-only">Search</span></button>
          </form>
        </li>

        <li class="nav-item mt-3">
          <div class="dropdown">
            <button
            class="translate dropdown-toggle border-0 rounded-50 ml-3"
            type="button"
            aria-expanded="false"
            aria-haspopup="true"
            data-toggle="dropdown"
            id="dropdown-menu-button"
          >
              {{Terms[4].Text}}
              <svg aria-hidden="true" class="header-language-arrow" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 30 19"><path d="M0.7,2.2l1.5-1.4C2.7,0.2,3.2,0,3.9,0s1.2,0.2,1.7,0.7L15,10l9.4-9.3c0.5-0.5,1-0.7,1.8-0.7
                c0.7,0,1.3,0.2,1.8,0.7l1.4,1.4C29.8,2.7,30,3.2,30,3.9s-0.2,1.3-0.7,1.8L16.7,18.2c-0.5,0.5-1.1,0.7-1.8,0.7s-1.3-0.2-1.8-0.7
                L0.7,5.7C0.2,5.2,0,4.6,0,3.9S0.2,2.7,0.7,2.2z"></path></svg>
            </button>
            <div class="dropdown-menu" aria-labelledby="dropdown-menu-button">
              {% for item in page | getAltPageRows %}
                <a class="dropdown-item" rel="alternate" lang="{{item.langcode}}" hreflang="{{item.langcode}}" href="{{item.url}}">{{item.langname}}</a>
              {% endfor %}
              <a class="dropdown-item" href="{{homelinkpath}}translate/">{{Terms[6].Text}}</a>
            </div>
          </div>
        </li>

      </ul>
    </div>
  </nav>

<div class="container-fluid bg-secondary-covid">
<nav class="container navbar navbar-expand-md navbar-light">
  
  <button class="navbar-toggler px-0 w-100" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
    <div class="navbar-brand d-sm-block d-md-none float-left ml-0 text-primary text-larger bold" href="#" style="">Menu</div><div class="ca-gov-icon-menu h1 text-primary text-right mr-0 float-right display-4"></div>
  </button>
  <div class="collapse navbar-collapse art-menu-full" id="navbarNavDropdown">
    <ul class="navbar-nav flex-row text-primary text-larger justify-content-between top-nav">
      {%- for section in menuData.sections -%}
      {%- set sectionNameRecord = pageNames | find("lang-en", section.title) -%}
      {%- set sectionTitle = sectionNameRecord[langwptag] | default(section.title) -%}
      {%- if section.links -%}
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle" 
            href="#"
            type="button" 
            aria-expanded="false" 
            aria-haspopup="true" 
            data-toggle="dropdown" 
            id="dropdown-menu-button-{{loop.index}}">
            {{sectionTitle}}
          </a>
          <div class="dropdown-menu" id="dropdown-{{loop.index}}" aria-labelledby="dropdown-menu-button-{{loop.index}}">
            {%- for link in section.links -%}
              {%- set href = link.href | default(homelinkpath+link.slug+"/#top")-%}
              {%- if link.slug -%}
                {%- set pageNameRecord = pageNames | find("slug", link.slug) -%}
                {%- set pageName = pageNameRecord[langwptag] | default(pageNameRecord["lang-en"]) | default(link.slug)  -%}
              {%- else -%}
                {%- set pageNameRecord = pageNames | find("href", href) -%}
                {%- set pageName = pageNameRecord[langwptag] | default(pageNameRecord["lang-en"]) | default(href) -%}
              {%- endif -%}
              <a class="dropdown-item" href="{{href}}">{{pageName}}</a>
            {%- endfor -%}
          </div>
        </li>
      {%- else -%}
        <li class="nav-item">
          {%- set href = section.href -%}
          {%- if href === "/" -%}
          {%- set href = homelinkpath -%}
          {%- endif %}
          <a class="nav-link" href="{{href}}">{{sectionTitle}}</a>
        </li>
      {%- endif -%}
    {%- endfor -%}
    </ul>
  </div>
</nav>
</div>

</header>
